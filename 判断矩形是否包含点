#include<stdio.h>
int main(void)
{
	double x1,y1,x2,y2,x3,y3,x4,y4,x,y;
	printf("input a,b,c,d:\n");
	scanf("%lf%lf%lf%lf%lf%lf%lf%lf",&x1,&y1,&x2,&y2,&x3,&y3,&x4,&y4);
	printf("input point:\n");
	scanf("%lf%lf",&x,&y);
	if(x>=x1&&x<=x4&&y>=y1&&y<=y4)
	 printf("yes");
	else
	 printf("no");
	return 0;
}
